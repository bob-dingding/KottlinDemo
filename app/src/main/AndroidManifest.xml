        <activity android:name=".classandobject.InterfaceActivity"/> <!-- 接口 -->
        <activity android:name=".classandobject.VisiableModifierActivity"/> <!-- 可见修饰符 -->
        <activity android:name=".classandobject.ExtensionActivity"/>
